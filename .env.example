APP_NAME=Calander API
PORT=3000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/calander-db

# API
API_PREFIX=/api
CORS_ORIGIN=*
# JWT
JWT_SECRET=your_super_secret_jwt_key_here_at_least_32_characters
JWT_EXPIRES_IN=1h
RESET_TOKEN_EXPIRES_IN=15m

# Security
MAX_REQUEST_SIZE=10485760
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
RATE_LIMIT_AUTH_MAX=5

